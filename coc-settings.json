{
  "languageserver": {
    "haskell": {
      "trace.server": "verbose",
      "command": "hie-wrapper",
      "args": ["--lsp", "-d", "-l", "/home/jule/lang-server.log"],
      "rootPatterns": [
        "stack.yaml",
        "cabal.config",
        "cabal.project",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": false
        }
      }
    }
  },
  "coc.preferences.hoverTarget": "float",
  "suggest.floatEnable": true
}
